{
  "name": "lydos-view",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "bun run typecheck && vite build",
    "typecheck": "tsc -b --noEmit",
    "lint": "biome check --write",
    "format": "biome format --write .",
    "format:check": "biome format .",
    "preview": "vite preview",
    "generate:api": "curl -k https://local.api.lydos/doc -o src/lib/openapi.json && openapi-typescript src/lib/openapi.json -o src/lib/api-types.ts"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.60.0",
    "@clerk/localizations": "^3.35.2",
    "@tanstack/react-query": "^5.90.20",
    "@tanstack/react-router": "^1.157.15",
    "clsx": "^2.1.1",
    "lucide-react": "^0.563.0",
    "openapi-fetch": "^0.15.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@eslint/js": "^9.39.1",
    "@tanstack/router-plugin": "^1.157.15",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.23",
    "babel-plugin-react-compiler": "^1.0.0",
    "eslint": "^9.39.1",
    "eslint-plugin-react-hooks": "^7.0.1",
    "eslint-plugin-react-refresh": "^0.4.24",
    "globals": "^16.5.0",
    "openapi-typescript": "^7.10.1",
    "postcss": "^8.5.6",
    "tailwindcss": "3.4.17",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.46.4",
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  }
}
